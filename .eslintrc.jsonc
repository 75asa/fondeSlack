{
  // "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "extends": [
    "airbnb-base",
    "prettier/@typescript-eslint"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "es6",
        "arrowParens": "avoid"
      }
    ]
  }
}
