{
  "name": "fondeSlack",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/nasat75/fondeSlack",
  "author": "Asato Nago <nago@tam-bourine.co.jp>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "npx tsc && node dist/src/index.js",
    "dev": "nodemon --watch './src/*.ts' --exec 'ts-node' src/index.ts",
    "serve": "node --require dotenv/config dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gcp-build": "npx tsc",
    "prestart": "yarn gcp-build",
    "deploy": "gcloud app deploy app.yml",
    "logs": "gcloud app logs",
    "logs:tail": "gcloud app logs tail",
    "lint": "eslint './src/**/*.ts'",
    "lint:fix": "eslint --fix './src/**/*.ts'"
  },
  "devDependencies": {
    "@types/eslint": "^6.8.1",
    "@types/node": "^14.0.11",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@slack/bolt": "^2.1.1",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.12.1",
    "ts-node": "^8.10.2"
  }
}
